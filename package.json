{
  "name": "blackbox",
  "version": "2.0.0",
  "description": "Professional webpage performance analysis tool for hackathon presentation",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:extension": "webpack --config webpack.extension.config.js --mode=production",
    "build:script": "webpack --config webpack.script.config.js --mode=production",
